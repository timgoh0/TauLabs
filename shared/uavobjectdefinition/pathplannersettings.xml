<xml>
	<object name="PathPlannerSettings" singleinstance="true" settings="true">
		<description>Settings for the @ref PathPlanner Module</description>
		<field name="PreprogrammedPath" units="" type="enum" elements="1" options="None,10m_Box,Logo,Star" defaultvalue="None"/>
		<field name="PlannerAlgorithm" units="" type="enum" elements="1" options="Direct,DirectWithFilleting" defaultvalue="Direct"/>
		<field name="PreferredRadius" units="m" type="float" elements="1" defaultvalue="0"/>
		<field name="FlashOperation" units="" type="enum" elements="1" defaultvalue="NONE">
			<options>
 				<option>NONE</option>
				<option>FAILED</option>
				<option>COMPLETED</option>
				<option>SAVE1</option>
				<option>SAVE2</option>
				<option>SAVE3</option>
				<option>SAVE4</option>
				<option>SAVE5</option>
				<option>LOAD1</option>
				<option>LOAD2</option>
				<option>LOAD3</option>
				<option>LOAD4</option>
				<option>LOAD5</option>
			</options>
		</field>
		<access gcs="readwrite" flight="readwrite"/>
		<telemetrygcs acked="true" updatemode="onchange" period="0"/>
		<telemetryflight acked="true" updatemode="onchange" period="0"/>
		<logging updatemode="manual" period="0"/>
	</object>
</xml>
